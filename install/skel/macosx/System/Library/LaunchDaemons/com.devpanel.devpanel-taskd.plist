<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.devpanel.taskd</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/webenabled/sbin/taskd</string>
        <string>-F</string>
    </array>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/webenabled/taskd.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/webenabled/taskd.log</string>
</dict>
</plist>
