/var/log/httpd/virtwww/*/*log {
    weekly
    rotate 8
    missingok
    notifempty
    sharedscripts
    delaycompress
    postrotate
        /sbin/service httpd reload > /dev/null 2>/dev/null || true ; \
        /opt/webenabled/libexec/fix-vhost-logs-ownshp-n-perms -y ; \
    endscript
}

/var/log/httpd/cgi_log {
        weekly
        missingok
        rotate 4
        compress
        delaycompress
        notifempty
        create 640 root apache
}
